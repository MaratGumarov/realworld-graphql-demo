type Query {
    articleById(id: ID): Article
    findArticleByIds(ids: [ID]): [Article]
    findArticleByIdWithAuthor(id: ID): Article
    findArticleByIdsWithAuthor(ids: [ID]): [Article]
    articlesByAuthor(id: ID): [Article]
}

type Article{
    text: String
    id: ID
    description: String
    author: Author
    date: String
}

type Author {
    id: ID
    firstName: String
    lastName: String
}